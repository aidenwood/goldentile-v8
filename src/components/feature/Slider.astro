<!-- CSS -->
<link rel="stylesheet" href="https://unpkg.com/flickity@2/dist/flickity.min.css">

<!-- JavaScript -->
<script src="https://unpkg.com/flickity@2/dist/flickity.pkgd.min.js"></script>

<!-- Perfect Flickity Secondary Page -->
<style>
  .carousel-cell {
  margin-right: 15px;
  width: 65%;
  height: 720px;
  max-width: 1440px;
  max-height: 80vh;
}

@media only screen and (max-width: 1680px) {
  .carousel-cell {
    width: 80%;
  }
}

@media only screen and (max-width: 1440px) {
  .carousel-cell {
    height: 460px;
  }
}

@media only screen and (max-width: 768px) {
  .carousel-cell {
    width: 95%;
    height: 560px;
  }
}

.carousel-cell.size-vertical { width: 35%; }

@media only screen and (max-width: 1440px) {
  .carousel-cell.size-vertical {
    width: 50%;
  }
}

@media only screen and (max-width: 768px) {
  .carousel-cell.size-vertical {
    width: 65%;
  }
}
</style>

<section class="py-4 sm:py-8 mb-4">
  
  <div class="mx-auto px-4 sm:px-6 w-full lg:px-8">
    <div class="carousel" data-flickity='{ "freeScroll": true, "contain": true, "autoPlay": 1400, "groupCells": "33%" }'>

      <div class="carousel-cell rounded-3xl bg-cover h-full" style="background-image: url(/assets/feature-wall/feature-0.webp); background-position: center;"></div>

      <div class="carousel-cell size-vertical rounded-3xl bg-cover h-full" style="background-image: url(/assets/feature-wall/feature-1.webp); background-position: center;"></div>

      <div class="carousel-cell size-vertical rounded-3xl bg-cover h-full" style="background-image: url(/assets/feature-wall/feature-2.webp); background-position: center;"></div>

      <div class="carousel-cell rounded-3xl bg-cover h-full" style="background-image: url(/assets/feature-wall/feature-3.webp); background-position: center;"></div>
      
      <div class="carousel-cell rounded-3xl bg-cover h-full" style="background-image: url(/assets/feature-wall/feature-4.webp); background-position: center;"></div>

      <div class="carousel-cell size-vertical rounded-3xl bg-cover h-full" style="background-image: url(/assets/feature-wall/feature-5.webp); background-position: center;"></div>
      
      <div class="carousel-cell  rounded-3xl bg-cover h-full" style="background-image: url(/assets/feature-wall/feature-7.webp); background-position: center;"></div>
      
      <div class="carousel-cell rounded-3xl bg-cover h-full" style="background-image: url(/assets/feature-wall/feature-8.webp); background-position: center;"></div>
      
      <div class="carousel-cell rounded-3xl bg-cover h-full" style="background-image: url(/assets/feature-wall/feature-9.webp); background-position: center;"></div>

      <div class="carousel-cell  min-h-full rounded-3xl">
        <img
          class=" rounded-3xl"
          width="920"
          height="460"
          src="/assets/feature-wall/feature-10.webp"
          alt="Gold Coast Protective Tiling Services such as Screeding and Waterproofing - Professional Tiling by Golden Tile for a Gold Coast client"
        />
      </div>
      <!-- bg-cover effect needs atleast one image used at back for flickity to work -->
    </div>
  </div>
</section>
